[
  "kill",
  "tgkill",
  "pthread_kill",
  "exit",
  "_exit",
  "abort",
  "prctl",
  "ptrace"
].forEach(name => {
    const f = Module.findExportByName(null, name);
    if (!f) return;

    Interceptor.attach(f, {
        onEnter(args) {
            console.log("[ANTI]", name, "called");
        }
    });
});