
Interceptor.attach(
    Module.findExportByName(null, "android_dlopen_ext"),
    {
        onEnter(args) {
            this.path = args[0].readCString();
        },
        onLeave(ret) {
            if (this.path.includes("libzwfeyyadt.so")) {
                const m = Process.findModuleByName("libzwfeyyadt.so");
                console.log("loaded:", m.base);

                // 여기서 즉시 주소 후킹
            }
        }
    }
);