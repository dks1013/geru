Interceptor.attach(
    Module.findExportByName(null, "dlopen"),
    {
        onEnter(args) {
            this.path = args[0].readCString();
            this.retaddr = this.returnAddress;
        },
        onLeave() {
            if (this.path && this.path.indexOf("libzwfeyyadt.so") !== -1) {
                const caller = Process.findModuleByAddress(this.retaddr);
                console.log(
                    "[dlopen]",
                    this.path,
                    "called from",
                    caller ? caller.name : "unknown",
                    this.retaddr
                );
            }
        }
    }
);