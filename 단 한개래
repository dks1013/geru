Interceptor.attach(ptr("0x6B338980"), {
  onEnter(args) {
    console.log("=== IMGSF50Filter StretchBlt hook hit ===");
    console.log("return:", this.returnAddress);
    const bt = Thread.backtrace(this.context, Backtracer.ACCURATE)
      .map(a => DebugSymbol.fromAddress(a))
      .join("\n");
    console.log(bt);
  }
});