const m = Process.findModuleByName("IMGSF50Filter_64.dll");

if (!m) {
    console.log("module not found");
    return;
}

console.log("module base:", m.base, "size:", m.size);

Memory.scan(m.base, m.size, "83 BA 30 01 00 00 00", {
    onMatch(addr) {
        console.log("[FOUND] =>", addr);
    },
    onComplete() {
        console.log("Scan finished");
    }
});